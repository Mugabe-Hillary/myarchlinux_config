{
   "action": {
      "default_icon": "images/icons/popup.png",
      "default_popup": "html/popup.html"
   },
   "background": {
      "service_worker": "js/background.js"
   },
   "content_scripts": [ {
      "css": [ "css/content/opera-store.css" ],
      "js": [ "js/content/opera-store.js" ],
      "matches": [ "https://addons.opera.com/*/extensions/details/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_ext_description__",
   "externally_connectable": {
      "matches": [ "https://crosspilot.io/*" ]
   },
   "icons": {
      "128": "images/icons/128.png",
      "16": "images/icons/16.png",
      "32": "images/icons/32.png",
      "64": "images/icons/64.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "120",
   "name": "__MSG_ext_name__",
   "optional_host_permissions": [ "http://*/*", "https://*/*" ],
   "optional_permissions": [ "tabs", "downloads" ],
   "options_ui": {
      "open_in_tab": true,
      "page": "html/options.html"
   },
   "permissions": [ "alarms", "storage", "unlimitedStorage", "offscreen", "scripting", "sidePanel" ],
   "sandbox": {
      "pages": [ "html/sandbox.html" ]
   },
   "side_panel": {
      "default_path": "html/side-panel.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.2.1"
}
